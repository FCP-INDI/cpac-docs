{
  "name": "cpac-docs",
  "version": "1.8.8",
  "description": "cpac-docs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@tsconfig/recommended": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@yarnpkg/pnpify": "^4.1.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "cpx": "^1.5.0",
    "eslint": "^9.28.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-typescript": "^0.14.0",
    "globals": "^16.2.0",
    "jiti": "^2.4.2",
    "marked": "^15.0.12",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.22"
  },
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "build": "tsc --outDir \"${OUTDIR:-./dist}\" --target es6",
    "check-ts": "tsc --target ES2020 --noEmit",
    "copy": "sh -c 'cpx \"src/**/*.!(ts)\" \"${OUTDIR:-./dist}\"' && sh -c 'cpx \"versions.yaml\" \"./dist\"' && sh -c 'cpx \"index.html\" \"./dist\"'",
    "copy-and-build": "yarn copy && yarn build",
    "dev": "vite",
    "watch": "chokidar 'src/**/*' -c 'yarn copy-and-build'",
    "serve": "vite preview",
    "format": "prettier",
    "lint": "eslint"
  }
}
